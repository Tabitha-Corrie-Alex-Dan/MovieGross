   "source": [
    "import pandas as pd\n",
    "import json\n",
    "import pymongo\n",
    "import numpy as np\n",
    "from flask import Flask, jsonify, request\n",
    "from flask_pymongo import PyMongo"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "# https://docs.mongodb.com/manual/reference/default-mongodb-port/\n",
    "conn = 'mongodb://localhost:27017'\n",
    "client = pymongo.MongoClient(conn)\n",
    "\n",
    "\n",
    "# Define the 'imdb_DB' database in Mongo\n",
    "db = client.imdb_DB"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "year = ['2000','2001','2002','2003','2004','2005','2006','2007','2008','2009','2010','2011','2012','2013','2014','2015','2016', '2017', '2018', '2019', '2020', '2021']\n",
    "season = [\"Spring\",\"Summer\",\"Fall\",\"Winter\"]\n",
    "\n",
    "years = len(year)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "imdb = []"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "i=0\n",
    "\n",
    "\n",
    "for y in year:\n",
    "    i = i+1\n",
    "    url = 'https://www.boxofficemojo.com/season/by-year/'+y+'/?grossesOption=calendarGrosses'\n",
    "    imdb.append(pd.read_html(url))\n",
    "#     print(imdb)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "# imdb_df = pd.DataFrame()\n",
    "\n",
    "for i in range(1,22):\n",
    "    imdb[i] = imdb[i][0].assign(Year=year[i])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "movies = []\n",
    "for i in range(1,22):\n",
    "    movies.append(imdb[i])\n",
    "\n",
    "\n",
    "imdb_df = pd.concat(movies)\n",
    "imdb_df.reset_index(inplace=True)    \n",
    "imdb_df.rename(columns={'index':'index', 'Season':'season', 'Cumulative Gross':'cum_gross', '% of Year':'pct_of_year', 'Releases':'releases',\n",
    "       'Average':'average', '#1 Release':'num_one_release', 'Genre':'genre', 'Budget':'budget', 'Running Time':'runtime', 'Gross':'gross',\n",
    "       '% of Total':'pct_of_total', 'Year':'year'}, inplace=True)"
   ]
  },
